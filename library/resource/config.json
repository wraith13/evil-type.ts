{
    "repository": "https://github.com/wraith13/evil-type.ts",
    "maxLineLength": 140,
    "safeNumber": false,
    "safeInteger": false,
    "regexpFlags": "u",
    "regulateType": {
        "sortBy": "define",
        "merge": true
    },
    "lineUnbreakableTokens": {
        "heads": [
            "async"
        ],
        "tails": [
            "as",
            "=>",
            "="
        ]
    }
}
