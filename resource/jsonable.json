                                "type": "number"
                            },
                            {
                                "$type": "primitive-type",
                                "type": "string"
                            }
                        ]
                    }
                },
                "JsonableArray": {
                    "export": true,
                    "$type": "type",
                    "define": {
                        "$type": "array",
                        "items": {
                            "$ref": "Jsonable"
                        }
                    }
                },
                "JsonableObject": {
                    "export": true,
                    "$type": "dictionary",
                    "valueType": {
                        "$ref": "Jsonable"
                    }
                },
                "Jsonable": {
                    "export": true,
                    "$type": "type",
                    "define": {
                        "$type": "or",
                        "types": [
                            {
                                "$ref": "JsonableValue"
                            },
                            {
                                "$ref": "JsonableArray"
                            },
                            {
                                "$ref": "JsonableObject"
                            }
                        ]
                    }
                },
                "parse": {
                    "export": true,
                    "$type": "code",
                    "tokens": [
                        "const",
                        "parse",
                        "=",
                        "(json: string)",
                        ":",
                        "Jsonable",
                        "=>",
                        "JSON.parse(json)"
                    ]
                },
                "stringify": {
                    "export": true,
                    "$type": "code",
                    "tokens": [
                        "const",
                        "stringify",
                        "=",
                        "(value: Jsonable, replacer?: ((this: any, key: string, value: any) => any) | (number | string)[] | null, space?: string | number)",
                        ":",
                        "string",
                        "=>",
                        "JSON.stringify(value, replacer as any, space)"
                    ]
                },
                "JsonablePartial": {
                    "export": true,
                    "$type": "code",
                    "tokens": [
                        "type",
                        "JsonablePartial<Target extends JsonableObject>",
                        "=",
                        "{ [key in keyof Target]?: Target[key] }",
                        "&",
                        "JsonableObject"
                    ]
                }
            }
        }
    },
    "options": {
        "outputFile": "./generated/code/jsonable.ts",
        "indentUnit": 4,
        "indentStyle": "allman",
        "validatorOption": "full",
        "schema": {
            "outputFile": "./generated/schema/jsonable.json",
            "$id": "https://raw.githubusercontent.com/wraith13/evil-type.ts/master/generated/schema/jsonable.json#"
