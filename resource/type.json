{
    "$ref": "https://raw.githubusercontent.com/wraith13/evil-type.ts/master/resource/type-schema.json#",
    "defines": {
        "Types": {
            "export": true,
            "$type": "module",
            "members": {
                "schema": {
                    "export": true,
                    "$type": "value",
                    "value": {
                        "$type": "literal",
                        "literal": "https://raw.githubusercontent.com/wraith13/evil-type.ts/master/resource/type-schema.json#"
                    }
                },
                "TypeSchema": {
                    "export": true,
                    "$type": "interface",
                    "members": {
                        "$ref": {
                            "$type": "typeof",
                            "value": {
                                "$ref": "schema"
                            }
                        },
                        "defines": {
                            "$type": "dictionary",
                            "valueType": {
                                "$ref": "Definition"
                            }
                        },
                        "options": {
                            "$ref": "OutputOptions"
                        }
                    }
                },
                "OutputOptions": {
                    "export": true,
                    "$type": "interface",
                    "members": {
                        "outputFile?": {
                            "$type": "primitive-type",
                            "type": "string"
                        },
                        "indentUnit": {
                            "$type": "or",
                            "types": [
                                {
                                    "$type": "primitive-type",
                                    "type": "number"
                                },
                                {
                                    "$type": "literal",
                                    "literal": "\t"
                                }
                            ]
                        },
                        "indentStyle": {
                            "$ref": "IndentStyleType"
                        },
                        "validatorOption": {
                            "$ref": "ValidatorOptionType"
                        },
                        "maxLineLength?": {
                            "$type": "or",
                            "types": [
                                {
                                    "$type": "primitive-type",
                                    "type": "null"
                                },
                                {
                                    "$type": "primitive-type",
                                    "type": "number"
                                }
                            ]
                        }
                    }
                },
                "indentStyleTypeMember": {
                    "export": true,
                    "$type": "value",
                    "value": {
                        "$type": "literal",
                        "literal": [
                            "allman",
                            "egyptian"
                        ]
                    },
                    "validator": false
                },
                "IndentStyleType": {
                    "export": true,
                    "$type": "type",
                    "define": {
                        "$type": "itemof",
                        "value": {
                            "$ref": "indentStyleTypeMember"
                        }
                    }
                },
                "ValidatorOptionType": {
                    "export": true,
                    "$type": "type",
                    "define": {
                        "$type": "enum-type",
                        "members": [
                            "none",
                            "simple",
                            "full"
                        ]
                    }
                },
                "AlphaElement": {
                    "export": true,
                    "$type": "interface",
                    "members": {
                        "$type": {
                            "$type": "primitive-type",
                            "type": "string"
                        }
                    }
                },
                "AlphaDefinition": {
                    "export": true,
                    "$type": "interface",
                    "extends": [
                        { "$ref": "AlphaElement" }
                    ],
                    "members": {
                        "export?": {
                            "$type": "primitive-type",
                            "type": "boolean"
                        }
                    }
                },
                "Definition": {
                    "export": true,
                    "$type": "type",
                    "define": {
                        "$type": "or",
                        "types": [
                            {
                                "$ref": "ModuleDefinition"
                            },
                            {
                                "$ref": "ValueDefinition"
                            },
                            {
                                "$ref": "TypeDefinition"
                            },
                            {
                                "$ref": "InterfaceDefinition"
                            }
                        ]
                    }
                },
                "ModuleDefinition": {
                    "export": true,
                    "$type": "interface",
                    "extends": [
                        {
                            "$ref": "AlphaDefinition"
                        }
                    ],
                    "members": {
                        "$type": {
                            "$type": "literal",
                            "literal": "module"
                        },
                        "members": {
                            "$type": "dictionary",
                            "valueType": {
                                "$ref": "Definition"
                            }
                        }
                    }
                },
                "ValueDefinition": {
                    "export": true,
                    "$type": "interface",
                    "extends": [
                        {
                            "$ref": "AlphaDefinition"
                        }
                    ],
                    "members": {
                        "$type": {
                            "$type": "literal",
                            "literal": "value"
                        },
                        "value": {
                            "$type": "or",
                            "types": [
                                {
                                    "$ref": "LiteralElement"
                                },
                                {
                                    "$ref": "ReferElement"
                                }
                            ]
                        },
                        "validator?": {
                            "$type": "primitive-type",
                            "type": "boolean"
                        }
                    }
                },
                "TypeDefinition": {
                    "export": true,
                    "$type": "interface",
                    "extends": [
                        {
                            "$ref": "AlphaDefinition"
                        }
                    ],
                    "members": {
                        "$type": {
                            "$type": "literal",
                            "literal": "type"
                        },
                        "define": {
                            "$ref": "TypeOrInterfaceOrRefer"
                        }
                    }
                },
                "InterfaceDefinition": {
                    "export": true,
                    "$type": "interface",
                    "extends": [
                        {
                            "$ref": "AlphaDefinition"
                        }
                    ],
                    "members": {
                        "$type": {
                            "$type": "literal",
                            "literal": "interface"
                        },
                        "extends?": {
                            "$type": "array",
                            "items": {
                                "$ref": "ReferElement"
                            }
                        },
                        "members": {
                            "$type": "dictionary",
                            "valueType": {
                                "$ref": "TypeOrInterfaceOrRefer"
                            }
                        }
                    }
                },
                "LiteralElement": {
                    "export": true,
                    "$type": "interface",
                    "extends": [
                        {
                            "$ref": "AlphaElement"
                        }
                    ],
                    "members": {
                        "$type": {
                            "$type": "literal",
                            "literal": "literal"
                        },
                        "literal": {
                            "$ref": "Jsonable"
                        }
                    }
                },
                "ReferElement": {
                    "export": true,
                    "$type": "interface",
                    "members": {
                        "$ref": {
                            "$type": "primitive-type",
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "options": {
        "outputFile": "./generated/types.ts",
        "indentUnit": 4,
        "indentStyle": "allman",
        "validatorOption": "full"
    }
}
