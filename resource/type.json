{
    "$ref": "https://raw.githubusercontent.com/wraith13/evil-type.ts/master/resource/type-schema.json#",
    "defines": {
        "schema": {
            "export": true,
            "$type": "value",
            "value": {
                "$type": "literal",
                "literal": "https://raw.githubusercontent.com/wraith13/evil-type.ts/master/resource/type-schema.json#"
            }
        },
        "TypeSchema": {
            "export": true,
            "$type": "interface",
            "members": {
                "$ref": {
                    "$type": "typeof",
                    "value": {
                        "$ref": "schema"
                    }
                },
                "defines": {
                    "$type": "interface",
                    "members": {}
                },
                "options": {
                    "$ref": "TypeOptions"
                }
            }
        },
        "TypeOptions": {
            "export": true,
            "$type": "interface",
            "members": {
                "indentUnit": {
                    "$type": "or",
                    "types": [
                        {
                            "$type": "primitive-type",
                            "type": "number"
                        },
                        {
                            "$type": "literalx",
                            "literal": "\t"
                        }
                    ]
                },
                "indentStyle": {
                    "$ref": "IndentStyleType"
                },
                "validatorOption": {
                    "$ref": "ValidatorOptionType"
                }
            }
        },
        "indentStyleTypeMember":
        {
            "export": true,
            "$type": "value",
            "value": {
                "$type": "literal",
                "literal": [
                    "allman",
                    "egyptian"
                ]
            }
        },
        "IndentStyleType": {
            "export": true,
            "$type": "type",
            "define": {
                "$type": "itemof",
                "value": {
                    "$ref": "indentStyleTypeMember"
                }
            }
        },
        "ValidatorOptionType": {
            "export": true,
            "$type": "type",
            "define": {
                "$type": "enum-type",
                "members": [
                    "none",
                    "simple",
                    "full"
                ]
            }
        }
    },
    "options": {
        "indentUnit": 4,
        "indentStyle": "allman",
        "validatorOption": "full"
    }
}